{
  "name": "API.AI with Slack integration",
  "description": "Api.ai Slack integration allows you to create Slack bots with natural language understanding based on Api.ai technology.",
  "repository": "https://github.com/xVir/api-ai-slack",
  "logo": "http://xvir.github.io/img/apiai.png",
  "keywords": ["api.ai", "slack", "natural language"],
  "env": {
    "APIAI_ACCESS_TOKEN": {
      "description": "Client access token for Api.ai",
      "value": ""
    },
    "SLACK_CLIENT_ID": {
      "description": "Slack bot client ID",
      "value": ""
    },
    "SLACK_CLIENT_SECRET": {
      "description": "Slack bot client secret",
      "value": ""
    },
    "LANG": {
      "description" : "Agent language",
      "value": "en"
    }
  },
  "engines": {
    "node": "5.9.0"
  },
  "addons": ["mongolab"]
}
